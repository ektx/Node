<style>
	h1 {
		background: #f90;
		font-size: 32px;
	}

	main {
		color: red;
	}

	@keyframes a {
		from {
			color: white
		}
		to {
			color: blue
		}
	}
</style>


<template>
	<main>
		<h1>Hello {{ name }}!</h1>
		{{ for (let i = 0; i < 5; i++) { }}
			<p>Test!</p>			
		{{ } }}

		<p i-for="5">number</p>

		<p i-for="val in [1,2]">{{val}}</p>

		<p i-for="val in { name: 'zwl', age: 22}">
			<span>{{val.name}}</span>
		</p>

		<its-slot></its-slot>
	</main>
</template>

